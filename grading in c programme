#include<stdio.h>
main()
{
	int marks;
	marks=0;
	printf("enter ur marks");
	scanf("%d",&marks);
	if(marks>=85)
	{
		printf("grade A");
		}
		else if(marks>=70)
		{
			printf("grade B");
		}
		else if(marks>=55)
		{
			printf("grade C");
		}
		else if(marks>=40)
		{
			printf("grade D");
		}
		else 
		{
			printf("grade F");
		}
	
}
